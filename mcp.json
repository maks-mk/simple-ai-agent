  },
  "tavily-remote-mcp": {
    "command": "npx",
    "args": ["-y", "--quiet", "mcp-remote", "https://mcp.tavily.com/mcp/?tavilyApiKey=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"],
