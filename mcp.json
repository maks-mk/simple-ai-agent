{
  "filesystem": {
    "command": "mcp-server-filesystem",
    "args": ["."],
    "transport": "stdio",
    "enabled": true
  },
  "win-cli": {
    "command": "npx",
    "args": ["@simonb97/server-win-cli", "--config", "./win-cli-config.json"],
    "transport": "stdio",
    "enabled": true
  },
  "context7": {
    "type": "remote",
    "url": "https://mcp.context7.com/mcp",
    "headers": {
      "CONTEXT7_API_KEY": "ctx7sk-........................"
    },
	"transport": "streamable_http",
    "enabled": false
  },
  "git": {
      "command": "uvx",
      "args": ["mcp-server-git"],
    "transport": "stdio",
    "enabled": false
	}
}