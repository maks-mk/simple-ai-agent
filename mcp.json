{
  "filesystem": {
    "command": "mcp-server-filesystem",
    "args": ["."],
    "transport": "stdio",
    "enabled": false
  },
   "mcp-cli-exec": {
      "command": "mcp-cli-exec",
      "args": [""],
      "transport": "stdio",
	  "enabled": false
	},
  "context7": {
    "type": "remote",
    "url": "https://mcp.context7.com/mcp",
    "transport": "http",
    "enabled": false
  },
  "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
	"transport": "stdio",
    "enabled": false
    },
  "pdf-tools": {
    "command": "uv",
	"args": [
      "run",
      "./pdf_mcp/pdf_server.py"
    ],
	"transport": "stdio",
    "enabled": false
  }
}