{
  "filesystem": {
    "command": "mcp-server-filesystem",
    "args": ["."],
    "transport": "stdio",
    "enabled": true
  },
  "win-cli": {
    "command": "npx",
    "args": ["@simonb97/server-win-cli", "--config", "./win-cli-config.json"],
